<script language="javascript">         	
    function toDetail(parameterId){
		$("#tab_online_tbody").html("");
		$("#tab_config_tbody").html("");
		$("#tab_log_tbody").html("");
    	jQuery.ajax({
    		type: "GET",
    		url: basePath + "/returnExchange/detail",
    		dataType : 'json',
    		data: "id=" + parameterId,
    		
    		success: function(data){
    			if(data!=null && data.code==200 && data.result!=null){
    				var r = data.result;
    				$("#r_id").text(r.id==null?"":r.id);
    				$("#r_serviceNo").text(r.serviceNo==null?"":r.serviceNo);
    				$("#r_pickupNo").text(r.pickupNo==null?"":r.pickupNo);
    				$("#r_serviceAuditStatus").text(r.serviceAuditStatus==null?"":r.serviceAuditStatus);
    				$("#r_orderNo").text(r.orderNo==null?"":r.orderNo);
    				$("#r_categoryId").text(r.categoryId==null?"":r.categoryId);
    				$("#r_productId").text(r.productId==null?"":r.productId);
    				$("#r_skuNo").text(r.skuNo==null?"":r.skuNo);
    				$("#r_skuName").text(r.skuName==null?"":r.skuName);
    				$("#r_serviceType").text(r.serviceType==null?"":r.serviceType);
    				$("#r_quantity").text(r.quantity==null?"":r.quantity);
    				$("#r_evidenceType").text(r.evidenceType==null?"":r.evidenceType);
    				$("#r_problemDescription").text(r.problemDescription==null?"":r.problemDescription);
    				$("#r_pickupType").text(r.pickupType==null?"":r.pickupType);
    				$("#r_pickupProvinceId").text(r.pickupProvinceId==null?"":r.pickupProvinceId);
    				$("#r_pickupProvinceName").text(r.pickupProvinceName==null?"":r.pickupProvinceName);
    				$("#r_pickupCityId").text(r.pickupCityId==null?"":r.pickupCityId);
    				$("#r_pickupCityName").text(r.pickupCityName==null?"":r.pickupCityName);
    				$("#r_pickupCountyId").text(r.pickupCountyId==null?"":r.pickupCountyId);
    				$("#r_pickupCuountyName").text(r.pickupCuountyName==null?"":r.pickupCuountyName);
    				$("#r_pickupDetailedAddress").text(r.pickupDetailedAddress==null?"":r.pickupDetailedAddress);
    				$("#r_reservationPickupTime").text(r.reservationPickupTime==null?"":r.reservationPickupTime);
    				$("#r_provinceId").text(r.provinceId==null?"":r.provinceId);
    				$("#r_provinceName").text(r.provinceName==null?"":r.provinceName);
    				$("#r_cityId").text(r.cityId==null?"":r.cityId);
    				$("#r_cityName").text(r.cityName==null?"":r.cityName);
    				$("#r_countyId").text(r.countyId==null?"":r.countyId);
    				$("#r_cuountyName").text(r.cuountyName==null?"":r.cuountyName);
    				$("#r_detailedAddress").text(r.detailedAddress==null?"":r.detailedAddress);
    				$("#r_consignee").text(r.consignee==null?"":r.consignee);
    				$("#r_mobileNo").text(r.mobileNo==null?"":r.mobileNo);
    				$("#r_telephoneNo").text(r.telephoneNo==null?"":r.telephoneNo);
    				$("#r_expressName").text(r.expressName==null?"":r.expressName);
    				$("#r_expressNo").text(r.expressNo==null?"":r.expressNo);
    				$("#r_waybillNo").text(r.waybillNo==null?"":r.waybillNo);
    				$("#r_createTime").text(r.createTime==null?"":r.createTime);
    				$("#r_updateTime").text(r.updateTime==null?"":r.updateTime);
    				$("#r_createUserId").text(r.createUserId==null?"":r.createUserId);
    				$("#r_createUser").text(r.createUser==null?"":r.createUser);
    				$("#r_updateUserId").text(r.updateUserId==null?"":r.updateUserId);
    				$("#r_updateUser").text(r.updateUser==null?"":r.updateUser);
    				$("#r_yn").text(r.yn==null?"":r.yn);
    			} else {
    				alert("获取详情失败");
    			}
    		},
    		
    		error : function(data) {
    			alert("获取详情出现异常");
    		}
    	});
		
    	$('#myReturnExchangeModal').modal('show');
    }
</script>