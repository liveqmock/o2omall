<script language="javascript">         	
    function toDetail(parameterId){
		$("#tab_online_tbody").html("");
		$("#tab_config_tbody").html("");
		$("#tab_log_tbody").html("");
    	jQuery.ajax({
    		type: "GET",
    		url: basePath + "/userProfile/detail",
    		dataType : 'json',
    		data: "id=" + parameterId,
    		
    		success: function(data){
    			if(data!=null && data.code==200 && data.result!=null){
    				var r = data.result;
    				$("#r_id").text(r.id==null?"":r.id);
    				$("#r_userId").text(r.userId==null?"":r.userId);
    				$("#r_cnname").text(r.cnname==null?"":r.cnname);
    				$("#r_sex").text(r.sex==null?"":r.sex);
    				$("#r_nickname").text(r.nickname==null?"":r.nickname);
    				$("#r_email").text(r.email==null?"":r.email);
    				$("#r_emailValidated").text(r.emailValidated==null?"":r.emailValidated);
    				$("#r_birthday").text(r.birthday==null?"":r.birthday);
    				$("#r_constellation").text(r.constellation==null?"":r.constellation);
    				$("#r_userPhoto").text(r.userPhoto==null?"":r.userPhoto);
    				$("#r_description").text(r.description==null?"":r.description);
    				$("#r_provinceName").text(r.provinceName==null?"":r.provinceName);
    				$("#r_provinceNo").text(r.provinceNo==null?"":r.provinceNo);
    				$("#r_cityName").text(r.cityName==null?"":r.cityName);
    				$("#r_cityNo").text(r.cityNo==null?"":r.cityNo);
    				$("#r_countyName").text(r.countyName==null?"":r.countyName);
    				$("#r_countyNo").text(r.countyNo==null?"":r.countyNo);
    				$("#r_address").text(r.address==null?"":r.address);
    				$("#r_mobile").text(r.mobile==null?"":r.mobile);
    				$("#r_phone").text(r.phone==null?"":r.phone);
    				$("#r_safeQuestion").text(r.safeQuestion==null?"":r.safeQuestion);
    				$("#r_safeAnswer").text(r.safeAnswer==null?"":r.safeAnswer);
    				$("#r_qq").text(r.qq==null?"":r.qq);
    				$("#r_grade").text(r.grade==null?"":r.grade);
    				$("#r_payPassword").text(r.payPassword==null?"":r.payPassword);
    				$("#r_score").text(r.score==null?"":r.score);
    				$("#r_createTime").text(r.createTime==null?"":r.createTime);
    				$("#r_updateTime").text(r.updateTime==null?"":r.updateTime);
    				$("#r_createUser").text(r.createUser==null?"":r.createUser);
    				$("#r_updateUser").text(r.updateUser==null?"":r.updateUser);
    				$("#r_yn").text(r.yn==null?"":r.yn);
    			} else {
    				alert("获取详情失败");
    			}
    		},
    		
    		error : function(data) {
    			alert("获取详情出现异常");
    		}
    	});
		
    	$('#myUserProfileModal').modal('show');
    }
</script>