<!DOCTYPE html>
<html lang="zh-cn">
    <head>
        #parse("/layout/head.vm")
		<script language="javascript"> 
			$(function() {
				$("#btn-query").click(function() {
        			pageSubmit(1);
   				});
				$("#btn-add").click(function() {
        			window.location.href=("$basePath/userProfile/addForward");
   				});
			});
        	// 提交分页查询
        	function pageSubmit(pageNoParam) {
                $("#curPage").val(pageNoParam);
            	$("#queryForm").attr("action", "$basePath/userProfile");
                $("#queryForm").attr("method", "POST");
                $("#queryForm").submit();
        	}
        	
        	function toUpdate(parameterId){
        		window.location.href=("$basePath/userProfile/updateForward?id="+parameterId);
        	}
        	
        	function toDelete(parameterId){
        		MyPopWindow.ExecuteDelete("$basePath/userProfile/delete",{ id : parameterId });
        	}
        	
        </script>
	</head>
    <body>
        <div class="row">
            <div class="col-xs-12 col-sm-12">
                <div class="widget-box">
                    <div class="widget-header">
                        <h5 class="lighter">
                            <i class="icon-search green"></i>用户基本信息查询
                        </h5>
                        <div class="widget-toolbar">
                            <a href="#" data-action="collapse">
                                <i class="1 icon-chevron-up bigger-100"></i>
                            </a>
                        </div>
                    </div>
                    <div class="widget-body">
                        <div class="widget-main" style="background-color: #f5f5f5;padding:5px;">
                            <form class="form-horizontal" role="form" id="queryForm">
								<div class="form-group">
									<label class="col-sm-1 control-label no-padding-right" for="userId">用户ID</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="userId" id="userId" class="col-xs-10 col-sm-8" value="$!query.userId"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="cnname">姓名</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="cnname" id="cnname" class="col-xs-10 col-sm-8" value="$!query.cnname"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="sex">性别</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="sex" id="sex" class="col-xs-10 col-sm-8" value="$!query.sex"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="nickname">昵称</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="nickname" id="nickname" class="col-xs-10 col-sm-8" value="$!query.nickname"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="email">邮箱</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="email" id="email" class="col-xs-10 col-sm-8" value="$!query.email"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="emailValidated">用户是否验证邮箱</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="emailValidated" id="emailValidated" class="col-xs-10 col-sm-8" value="$!query.emailValidated"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="birthday">生日</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="birthday" id="birthday" class="col-xs-10 col-sm-8" value="$!query.birthday"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="constellation">星座</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="constellation" id="constellation" class="col-xs-10 col-sm-8" value="$!query.constellation"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="userPhoto">用户头像</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="userPhoto" id="userPhoto" class="col-xs-10 col-sm-8" value="$!query.userPhoto"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="description">自我介绍</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="description" id="description" class="col-xs-10 col-sm-8" value="$!query.description"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="provinceName">地区-省份-名称</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="provinceName" id="provinceName" class="col-xs-10 col-sm-8" value="$!query.provinceName"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="provinceNo">地区-省份-编号</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="provinceNo" id="provinceNo" class="col-xs-10 col-sm-8" value="$!query.provinceNo"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="cityName">地区-市-名称</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="cityName" id="cityName" class="col-xs-10 col-sm-8" value="$!query.cityName"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="cityNo">地区-市-编号</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="cityNo" id="cityNo" class="col-xs-10 col-sm-8" value="$!query.cityNo"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="countyName">地区-县-名称</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="countyName" id="countyName" class="col-xs-10 col-sm-8" value="$!query.countyName"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="countyNo">地区-县-编号</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="countyNo" id="countyNo" class="col-xs-10 col-sm-8" value="$!query.countyNo"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="address">联系地址</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="address" id="address" class="col-xs-10 col-sm-8" value="$!query.address"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="mobile">移动电话</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="mobile" id="mobile" class="col-xs-10 col-sm-8" value="$!query.mobile"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="phone">固定电话</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="phone" id="phone" class="col-xs-10 col-sm-8" value="$!query.phone"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="safeQuestion">安全问题</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="safeQuestion" id="safeQuestion" class="col-xs-10 col-sm-8" value="$!query.safeQuestion"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="safeAnswer">安全问题答案</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="safeAnswer" id="safeAnswer" class="col-xs-10 col-sm-8" value="$!query.safeAnswer"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="qq">QQ</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="qq" id="qq" class="col-xs-10 col-sm-8" value="$!query.qq"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="grade">会员等级</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="grade" id="grade" class="col-xs-10 col-sm-8" value="$!query.grade"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="payPassword">支付密码</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="payPassword" id="payPassword" class="col-xs-10 col-sm-8" value="$!query.payPassword"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="score">我的剩余积分</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="score" id="score" class="col-xs-10 col-sm-8" value="$!query.score"/>
                                    </div>
								</div>
								<div class="form-group">
									<div class="col-sm-4 col-sm-offset-8">
    									#parse("/layout/page.vm")
                                        <button class="btn btn-info btn-sm" type="button" id="btn-query"><i class="icon-search"></i>查询</button>
                                        &nbsp; &nbsp; &nbsp;
                                        <button class="btn btn-sm" type="reset" id="btn-reset"><i class="icon-undo"></i>重置</button>
										 &nbsp; &nbsp; &nbsp;
              							<button class="btn btn-success btn-sm" type="button" id="btn-add"><i class="icon-plus"></i>新增</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
			<div class="col-xs-12 col-sm-12 table-responsive">
                <table class="table table-striped table-bordered table-condensed table-hover">
                    <thead>
                        <tr>
							<th class="center">id自增</th>
							<th class="center">用户ID</th>
							<th class="center">姓名</th>
							<th class="center">性别</th>
							<th class="center">昵称</th>
							<th class="center">邮箱</th>
							<th class="center">用户是否验证邮箱</th>
							<th class="center">生日</th>
							<th class="center">星座</th>
							<th class="center">用户头像</th>
							<th class="center">自我介绍</th>
							<th class="center">地区-省份-名称</th>
							<th class="center">地区-省份-编号</th>
							<th class="center">地区-市-名称</th>
							<th class="center">地区-市-编号</th>
							<th class="center">地区-县-名称</th>
							<th class="center">地区-县-编号</th>
							<th class="center">联系地址</th>
							<th class="center">移动电话</th>
							<th class="center">固定电话</th>
							<th class="center">安全问题</th>
							<th class="center">安全问题答案</th>
							<th class="center">QQ</th>
							<th class="center">会员等级</th>
							<th class="center">支付密码</th>
							<th class="center">我的剩余积分</th>
							<th class="center">创建时间</th>
							<th class="center">创建人</th>
    						<th class="center">操作</th>
                        </tr>
                    </thead>
                    <tbody>
                        #foreach( $item in $dataList)
                            <tr>
                                <td>$!item.id</td>
                                <td>$!item.userId</td>
                                <td>$!item.cnname</td>
                                <td>$!item.sex</td>
                                <td>$!item.nickname</td>
                                <td>$!item.email</td>
                                <td>$!item.emailValidated</td>
                                <td>$!item.birthday</td>
                                <td>$!item.constellation</td>
                                <td>$!item.userPhoto</td>
                                <td>$!item.description</td>
                                <td>$!item.provinceName</td>
                                <td>$!item.provinceNo</td>
                                <td>$!item.cityName</td>
                                <td>$!item.cityNo</td>
                                <td>$!item.countyName</td>
                                <td>$!item.countyNo</td>
                                <td>$!item.address</td>
                                <td>$!item.mobile</td>
                                <td>$!item.phone</td>
                                <td>$!item.safeQuestion</td>
                                <td>$!item.safeAnswer</td>
                                <td>$!item.qq</td>
                                <td>$!item.grade</td>
                                <td>$!item.payPassword</td>
                                <td>$!item.score</td>
                                <td>$!dateTool.format('yyyy-MM-dd HH:mm:ss ',$!item.createTime)</td>
                                <td>$!item.createUser</td>
                                <td>
									<a href="javascript:void(0)" onclick="toDetail($!item.id);" data-toggle="modal"><i class="icon-list">详情</i></a>
									<a href="javascript:void(0)" onclick="toUpdate($!item.id)"><i class="icon-pencil pink">修改</i></a>
									<a href="javascript:void(0)" onclick="toDelete($!item.id)"><i class="icon-remove red">删除</i></a>
								</td>
                            </tr>
                        #end
                    </tbody>
                </table>
				#pageBarPost($page.totalRow $page.curPage $page.totalPage)
			</div>
        </div>
		#parse("/userProfile/detailModal.vm")
    </body>
</html>
