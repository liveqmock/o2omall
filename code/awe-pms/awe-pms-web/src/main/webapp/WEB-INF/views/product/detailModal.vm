<script language="javascript">         	
    function toDetail(parameterId){
		$("#tab_online_tbody").html("");
		$("#tab_config_tbody").html("");
		$("#tab_log_tbody").html("");
    	jQuery.ajax({
    		type: "GET",
    		url: basePath + "/product/detail",
    		dataType : 'json',
    		data: "id=" + parameterId,
    		
    		success: function(data){
    			if(data!=null && data.code==200 && data.result!=null){
    				var r = data.result;
    				$("#r_id").text(r.id==null?"":r.id);
    				$("#r_productNo").text(r.productNo==null?"":r.productNo);
    				$("#r_productName").text(r.productName==null?"":r.productName);
    				$("#r_enName").text(r.enName==null?"":r.enName);
    				$("#r_keyword").text(r.keyword==null?"":r.keyword);
    				$("#r_categoryOneId").text(r.categoryOneId==null?"":r.categoryOneId);
    				$("#r_categoryOne").text(r.categoryOne==null?"":r.categoryOne);
    				$("#r_categoryTwoId").text(r.categoryTwoId==null?"":r.categoryTwoId);
    				$("#r_categoryTwo").text(r.categoryTwo==null?"":r.categoryTwo);
    				$("#r_categoryThreeId").text(r.categoryThreeId==null?"":r.categoryThreeId);
    				$("#r_categoryThree").text(r.categoryThree==null?"":r.categoryThree);
    				$("#r_skuAttrName").text(r.skuAttrName==null?"":r.skuAttrName);
    				$("#r_score").text(r.score==null?"":r.score);
    				$("#r_level").text(r.level==null?"":r.level);
    				$("#r_brandCode").text(r.brandCode==null?"":r.brandCode);
    				$("#r_brandName").text(r.brandName==null?"":r.brandName);
    				$("#r_type").text(r.type==null?"":r.type);
    				$("#r_typeName").text(r.typeName==null?"":r.typeName);
    				$("#r_mode").text(r.mode==null?"":r.mode);
    				$("#r_modeName").text(r.modeName==null?"":r.modeName);
    				$("#r_businessNo").text(r.businessNo==null?"":r.businessNo);
    				$("#r_businessName").text(r.businessName==null?"":r.businessName);
    				$("#r_method").text(r.method==null?"":r.method);
    				$("#r_originPlace").text(r.originPlace==null?"":r.originPlace);
    				$("#r_saleTimeStart").text(r.saleTimeStart==null?"":r.saleTimeStart);
    				$("#r_applicableCrowd").text(r.applicableCrowd==null?"":r.applicableCrowd);
    				$("#r_applicableAge").text(r.applicableAge==null?"":r.applicableAge);
    				$("#r_features").text(r.features==null?"":r.features);
    				$("#r_describe").text(r.describe==null?"":r.describe);
    				$("#r_introduce").text(r.introduce==null?"":r.introduce);
    				$("#r_service").text(r.service==null?"":r.service);
    				$("#r_weight").text(r.weight==null?"":r.weight);
    				$("#r_grossWeight").text(r.grossWeight==null?"":r.grossWeight);
    				$("#r_length").text(r.length==null?"":r.length);
    				$("#r_width").text(r.width==null?"":r.width);
    				$("#r_height").text(r.height==null?"":r.height);
    				$("#r_diameter").text(r.diameter==null?"":r.diameter);
    				$("#r_packType").text(r.packType==null?"":r.packType);
    				$("#r_packLength").text(r.packLength==null?"":r.packLength);
    				$("#r_packWidth").text(r.packWidth==null?"":r.packWidth);
    				$("#r_packHeight").text(r.packHeight==null?"":r.packHeight);
    				$("#r_supportReturn").text(r.supportReturn==null?"":r.supportReturn);
    				$("#r_distribution").text(r.distribution==null?"":r.distribution);
    				$("#r_freightFree").text(r.freightFree==null?"":r.freightFree);
