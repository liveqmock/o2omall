                                    <div class="col-sm-4">
                                        <input type="text" name="diameter" id="diameter" class="col-xs-12 col-sm-12" value="$!product.diameter" />
                                    </div>
									<label class="col-sm-offset-1 col-sm-1 control-label no-padding-right" for="packType">包装类型（桶装等）</label>
                                    <div class="col-sm-4">
                                        <input type="text" name="packType" id="packType" class="col-xs-12 col-sm-12" value="$!product.packType" />
                                    </div>
									<label class="col-sm-offset-1 col-sm-1 control-label no-padding-right" for="packLength">包装长</label>
                                    <div class="col-sm-4">
                                        <input type="text" name="packLength" id="packLength" class="col-xs-12 col-sm-12" value="$!product.packLength" />
                                    </div>
									<label class="col-sm-offset-1 col-sm-1 control-label no-padding-right" for="packWidth">包装宽</label>
                                    <div class="col-sm-4">
                                        <input type="text" name="packWidth" id="packWidth" class="col-xs-12 col-sm-12" value="$!product.packWidth" />
                                    </div>
									<label class="col-sm-offset-1 col-sm-1 control-label no-padding-right" for="packHeight">包装高</label>
                                    <div class="col-sm-4">
                                        <input type="text" name="packHeight" id="packHeight" class="col-xs-12 col-sm-12" value="$!product.packHeight" />
                                    </div>
									<label class="col-sm-offset-1 col-sm-1 control-label no-padding-right" for="supportReturn">是否支持退货</label>
                                    <div class="col-sm-4">
                                        <input type="text" name="supportReturn" id="supportReturn" class="col-xs-12 col-sm-12" value="$!product.supportReturn" />
                                    </div>
									<label class="col-sm-offset-1 col-sm-1 control-label no-padding-right" for="distribution">配送信息</label>
                                    <div class="col-sm-4">
                                        <input type="text" name="distribution" id="distribution" class="col-xs-12 col-sm-12" value="$!product.distribution" />
                                    </div>
									<label class="col-sm-offset-1 col-sm-1 control-label no-padding-right" for="freightFree">是否免运费</label>
