<!DOCTYPE html>
<html lang="zh-cn">
    <head>
        #parse("/layout/head.vm")
		<script language="javascript"> 
			$(function() {
				$("#btn-query").click(function() {
        			pageSubmit(1);
   				});
				$("#btn-add").click(function() {
        			window.location.href=("$basePath/productBrand/addForward");
   				});
			});
        	// 提交分页查询
        	function pageSubmit(pageNoParam) {
                $("#curPage").val(pageNoParam);
            	$("#queryForm").attr("action", "$basePath/productBrand");
                $("#queryForm").attr("method", "POST");
                $("#queryForm").submit();
        	}
        	
        	function toUpdate(parameterId){
        		window.location.href=("$basePath/productBrand/updateForward?id="+parameterId);
        	}
        	
        	function toDelete(parameterId){
        		MyPopWindow.ExecuteDelete("$basePath/productBrand/delete",{ id : parameterId });
        	}
        	
        </script>
	</head>
    <body>
        <div class="row">
            <div class="col-xs-12 col-sm-12">
                <div class="widget-box">
                    <div class="widget-header">
                        <h5 class="lighter">
                            <i class="icon-search green"></i>商品类别品牌查询
                        </h5>
                        <div class="widget-toolbar">
                            <a href="#" data-action="collapse">
                                <i class="1 icon-chevron-up bigger-100"></i>
                            </a>
                        </div>
                    </div>
                    <div class="widget-body">
                        <div class="widget-main" style="background-color: #f5f5f5;padding:5px;">
                            <form class="form-horizontal" role="form" id="queryForm">
								<div class="form-group">
									<label class="col-sm-1 control-label no-padding-right" for="categoryOneId">一级分类</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="categoryOneId" id="categoryOneId" class="col-xs-10 col-sm-8" value="$!query.categoryOneId"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="categoryOne">一级分类名称</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="categoryOne" id="categoryOne" class="col-xs-10 col-sm-8" value="$!query.categoryOne"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="categoryTwoId">二级分类</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="categoryTwoId" id="categoryTwoId" class="col-xs-10 col-sm-8" value="$!query.categoryTwoId"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="categoryTwo">二级分类名称</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="categoryTwo" id="categoryTwo" class="col-xs-10 col-sm-8" value="$!query.categoryTwo"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="categoryThreeId">三级分类</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="categoryThreeId" id="categoryThreeId" class="col-xs-10 col-sm-8" value="$!query.categoryThreeId"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="categoryThree">三级分类名称</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="categoryThree" id="categoryThree" class="col-xs-10 col-sm-8" value="$!query.categoryThree"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="brandCode">品牌编号</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="brandCode" id="brandCode" class="col-xs-10 col-sm-8" value="$!query.brandCode"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="brandName">品牌名称</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="brandName" id="brandName" class="col-xs-10 col-sm-8" value="$!query.brandName"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="enName">品牌英文名称</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="enName" id="enName" class="col-xs-10 col-sm-8" value="$!query.enName"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="brandAbbr">品牌简称</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="brandAbbr" id="brandAbbr" class="col-xs-10 col-sm-8" value="$!query.brandAbbr"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="keyword">关键字</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="keyword" id="keyword" class="col-xs-10 col-sm-8" value="$!query.keyword"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="type">商品类型（国内、进口）</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="type" id="type" class="col-xs-10 col-sm-8" value="$!query.type"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="typeName">商品类型名称</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="typeName" id="typeName" class="col-xs-10 col-sm-8" value="$!query.typeName"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="provinceName2">地区-省份-名称</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="provinceName2" id="provinceName2" class="col-xs-10 col-sm-8" value="$!query.provinceName2"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="provinceNo">地区-省份-编号</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="provinceNo" id="provinceNo" class="col-xs-10 col-sm-8" value="$!query.provinceNo"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="cityName2">地区-市-名称</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="cityName2" id="cityName2" class="col-xs-10 col-sm-8" value="$!query.cityName2"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="cityNo">地区-市-编号</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="cityNo" id="cityNo" class="col-xs-10 col-sm-8" value="$!query.cityNo"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="priority">优先级</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="priority" id="priority" class="col-xs-10 col-sm-8" value="$!query.priority"/>
                                    </div>
								</div>
								<div class="form-group">
									<div class="col-sm-4 col-sm-offset-8">
    									#parse("/layout/page.vm")
                                        <button class="btn btn-info btn-sm" type="button" id="btn-query"><i class="icon-search"></i>查询</button>
                                        &nbsp; &nbsp; &nbsp;
                                        <button class="btn btn-sm" type="reset" id="btn-reset"><i class="icon-undo"></i>重置</button>
										 &nbsp; &nbsp; &nbsp;
              							<button class="btn btn-success btn-sm" type="button" id="btn-add"><i class="icon-plus"></i>新增</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
			<div class="col-xs-12 col-sm-12 table-responsive">
                <table class="table table-striped table-bordered table-condensed table-hover">
                    <thead>
                        <tr>
							<th class="center">主键</th>
							<th class="center">一级分类</th>
							<th class="center">一级分类名称</th>
							<th class="center">二级分类</th>
							<th class="center">二级分类名称</th>
							<th class="center">三级分类</th>
							<th class="center">三级分类名称</th>
							<th class="center">品牌编号</th>
							<th class="center">品牌名称</th>
							<th class="center">品牌英文名称</th>
							<th class="center">品牌简称</th>
							<th class="center">关键字</th>
							<th class="center">商品类型（国内、进口）</th>
							<th class="center">商品类型名称</th>
							<th class="center">地区-省份-名称</th>
							<th class="center">地区-省份-编号</th>
							<th class="center">地区-市-名称</th>
							<th class="center">地区-市-编号</th>
							<th class="center">优先级</th>
							<th class="center">创建时间</th>
							<th class="center">创建人</th>
    						<th class="center">操作</th>
                        </tr>
                    </thead>
                    <tbody>
                        #foreach( $item in $dataList)
                            <tr>
                                <td>$!item.id</td>
                                <td>$!item.categoryOneId</td>
                                <td>$!item.categoryOne</td>
                                <td>$!item.categoryTwoId</td>
                                <td>$!item.categoryTwo</td>
                                <td>$!item.categoryThreeId</td>
                                <td>$!item.categoryThree</td>
                                <td>$!item.brandCode</td>
                                <td>$!item.brandName</td>
                                <td>$!item.enName</td>
                                <td>$!item.brandAbbr</td>
                                <td>$!item.keyword</td>
                                <td>$!item.type</td>
                                <td>$!item.typeName</td>
                                <td>$!item.provinceName2</td>
                                <td>$!item.provinceNo</td>
                                <td>$!item.cityName2</td>
                                <td>$!item.cityNo</td>
                                <td>$!item.priority</td>
                                <td>$!dateTool.format('yyyy-MM-dd HH:mm:ss ',$!item.createTime)</td>
                                <td>$!item.createUser</td>
                                <td>
									<a href="javascript:void(0)" onclick="toDetail($!item.id);" data-toggle="modal"><i class="icon-list">详情</i></a>
									<a href="javascript:void(0)" onclick="toUpdate($!item.id)"><i class="icon-pencil pink">修改</i></a>
									<a href="javascript:void(0)" onclick="toDelete($!item.id)"><i class="icon-remove red">删除</i></a>
								</td>
                            </tr>
                        #end
                    </tbody>
                </table>
				#pageBarPost($page.totalRow $page.curPage $page.totalPage)
			</div>
        </div>
		#parse("/productBrand/detailModal.vm")
    </body>
</html>
