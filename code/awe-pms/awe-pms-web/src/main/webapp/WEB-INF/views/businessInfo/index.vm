<!DOCTYPE html>
<html lang="zh-cn">
    <head>
        #parse("/layout/head.vm")
		<script language="javascript"> 
			$(function() {
				$("#btn-query").click(function() {
        			pageSubmit(1);
   				});
				$("#btn-add").click(function() {
        			window.location.href=("$basePath/businessInfo/addForward");
   				});
			});
        	// 提交分页查询
        	function pageSubmit(pageNoParam) {
                $("#curPage").val(pageNoParam);
            	$("#queryForm").attr("action", "$basePath/businessInfo");
                $("#queryForm").attr("method", "POST");
                $("#queryForm").submit();
        	}
        	
        	function toUpdate(parameterId){
        		window.location.href=("$basePath/businessInfo/updateForward?id="+parameterId);
        	}
        	
        	function toDelete(parameterId){
        		MyPopWindow.ExecuteDelete("$basePath/businessInfo/delete",{ id : parameterId });
        	}
        	
        </script>
	</head>
    <body>
        <div class="row">
            <div class="col-xs-12 col-sm-12">
                <div class="widget-box">
                    <div class="widget-header">
                        <h5 class="lighter">
                            <i class="icon-search green"></i>商家信息查询
                        </h5>
                        <div class="widget-toolbar">
                            <a href="#" data-action="collapse">
                                <i class="1 icon-chevron-up bigger-100"></i>
                            </a>
                        </div>
                    </div>
                    <div class="widget-body">
                        <div class="widget-main" style="background-color: #f5f5f5;padding:5px;">
                            <form class="form-horizontal" role="form" id="queryForm">
								<div class="form-group">
									<label class="col-sm-1 control-label no-padding-right" for="businessNo">商家编号</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="businessNo" id="businessNo" class="col-xs-10 col-sm-8" value="$!query.businessNo"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="businessName">商家名称</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="businessName" id="businessName" class="col-xs-10 col-sm-8" value="$!query.businessName"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="enName">商家英文名称</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="enName" id="enName" class="col-xs-10 col-sm-8" value="$!query.enName"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="businessAbbr">名称缩写</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="businessAbbr" id="businessAbbr" class="col-xs-10 col-sm-8" value="$!query.businessAbbr"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="country">国家</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="country" id="country" class="col-xs-10 col-sm-8" value="$!query.country"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="company">公司</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="company" id="company" class="col-xs-10 col-sm-8" value="$!query.company"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="address">地址</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="address" id="address" class="col-xs-10 col-sm-8" value="$!query.address"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="postCode">邮编</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="postCode" id="postCode" class="col-xs-10 col-sm-8" value="$!query.postCode"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="contactor">联系人</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="contactor" id="contactor" class="col-xs-10 col-sm-8" value="$!query.contactor"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="telePhone">联系人电话</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="telePhone" id="telePhone" class="col-xs-10 col-sm-8" value="$!query.telePhone"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="mobilePhone">手机</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="mobilePhone" id="mobilePhone" class="col-xs-10 col-sm-8" value="$!query.mobilePhone"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="bussSort">商家类别</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="bussSort" id="bussSort" class="col-xs-10 col-sm-8" value="$!query.bussSort"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="describe">描述</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="describe" id="describe" class="col-xs-10 col-sm-8" value="$!query.describe"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="remark">备注</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="remark" id="remark" class="col-xs-10 col-sm-8" value="$!query.remark"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="checkStatus">审核状态</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="checkStatus" id="checkStatus" class="col-xs-10 col-sm-8" value="$!query.checkStatus"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="hasCard">是否有证件</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="hasCard" id="hasCard" class="col-xs-10 col-sm-8" value="$!query.hasCard"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="legalPerson">法定人身份证（图片地址）</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="legalPerson" id="legalPerson" class="col-xs-10 col-sm-8" value="$!query.legalPerson"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="companyRegister">公司注册信息（扫描件）</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="companyRegister" id="companyRegister" class="col-xs-10 col-sm-8" value="$!query.companyRegister"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="enterTime">入驻时间</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="enterTime" id="enterTime" class="col-xs-10 col-sm-8" value="$!query.enterTime"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="enterYear">入驻年限</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="enterYear" id="enterYear" class="col-xs-10 col-sm-8" value="$!query.enterYear"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="contractTimeStart">有效合同开始时间</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="contractTimeStart" id="contractTimeStart" class="col-xs-10 col-sm-8" value="$!query.contractTimeStart"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="contractTimeEnd">有效合同结束时间</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="contractTimeEnd" id="contractTimeEnd" class="col-xs-10 col-sm-8" value="$!query.contractTimeEnd"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="level">商家级别</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="level" id="level" class="col-xs-10 col-sm-8" value="$!query.level"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="earnestMoney">保证金</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="earnestMoney" id="earnestMoney" class="col-xs-10 col-sm-8" value="$!query.earnestMoney"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="creditLevel">信用等级</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="creditLevel" id="creditLevel" class="col-xs-10 col-sm-8" value="$!query.creditLevel"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="creditLimit">信用额度</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="creditLimit" id="creditLimit" class="col-xs-10 col-sm-8" value="$!query.creditLimit"/>
                                    </div>
									<label class="col-sm-1 control-label no-padding-right" for="priority">优先级</label>
                                    <div class="col-sm-3">
                                        <input type="text" name="priority" id="priority" class="col-xs-10 col-sm-8" value="$!query.priority"/>
                                    </div>
								</div>
								<div class="form-group">
									<div class="col-sm-4 col-sm-offset-8">
    									#parse("/layout/page.vm")
                                        <button class="btn btn-info btn-sm" type="button" id="btn-query"><i class="icon-search"></i>查询</button>
                                        &nbsp; &nbsp; &nbsp;
                                        <button class="btn btn-sm" type="reset" id="btn-reset"><i class="icon-undo"></i>重置</button>
										 &nbsp; &nbsp; &nbsp;
              							<button class="btn btn-success btn-sm" type="button" id="btn-add"><i class="icon-plus"></i>新增</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
			<div class="col-xs-12 col-sm-12 table-responsive">
                <table class="table table-striped table-bordered table-condensed table-hover">
                    <thead>
                        <tr>
							<th class="center">主键</th>
							<th class="center">商家编号</th>
							<th class="center">商家名称</th>
							<th class="center">商家英文名称</th>
							<th class="center">名称缩写</th>
							<th class="center">国家</th>
							<th class="center">公司</th>
							<th class="center">地址</th>
							<th class="center">邮编</th>
							<th class="center">联系人</th>
							<th class="center">联系人电话</th>
							<th class="center">手机</th>
							<th class="center">商家类别</th>
							<th class="center">描述</th>
							<th class="center">备注</th>
							<th class="center">审核状态</th>
							<th class="center">是否有证件</th>
							<th class="center">法定人身份证（图片地址）</th>
							<th class="center">公司注册信息（扫描件）</th>
							<th class="center">入驻时间</th>
							<th class="center">入驻年限</th>
							<th class="center">有效合同开始时间</th>
							<th class="center">有效合同结束时间</th>
							<th class="center">商家级别</th>
							<th class="center">保证金</th>
							<th class="center">信用等级</th>
							<th class="center">信用额度</th>
							<th class="center">优先级</th>
							<th class="center">创建时间</th>
							<th class="center">创建人</th>
    						<th class="center">操作</th>
                        </tr>
                    </thead>
                    <tbody>
                        #foreach( $item in $dataList)
                            <tr>
                                <td>$!item.id</td>
                                <td>$!item.businessNo</td>
                                <td>$!item.businessName</td>
                                <td>$!item.enName</td>
                                <td>$!item.businessAbbr</td>
                                <td>$!item.country</td>
                                <td>$!item.company</td>
                                <td>$!item.address</td>
                                <td>$!item.postCode</td>
                                <td>$!item.contactor</td>
                                <td>$!item.telePhone</td>
                                <td>$!item.mobilePhone</td>
                                <td>$!item.bussSort</td>
                                <td>$!item.describe</td>
                                <td>$!item.remark</td>
                                <td>$!item.checkStatus</td>
                                <td>$!item.hasCard</td>
                                <td>$!item.legalPerson</td>
                                <td>$!item.companyRegister</td>
                                <td>$!item.enterTime</td>
                                <td>$!item.enterYear</td>
                                <td>$!item.contractTimeStart</td>
                                <td>$!item.contractTimeEnd</td>
                                <td>$!item.level</td>
                                <td>$!item.earnestMoney</td>
                                <td>$!item.creditLevel</td>
                                <td>$!item.creditLimit</td>
                                <td>$!item.priority</td>
                                <td>$!dateTool.format('yyyy-MM-dd HH:mm:ss ',$!item.createTime)</td>
                                <td>$!item.createUser</td>
                                <td>
									<a href="javascript:void(0)" onclick="toDetail($!item.id);" data-toggle="modal"><i class="icon-list">详情</i></a>
									<a href="javascript:void(0)" onclick="toUpdate($!item.id)"><i class="icon-pencil pink">修改</i></a>
									<a href="javascript:void(0)" onclick="toDelete($!item.id)"><i class="icon-remove red">删除</i></a>
								</td>
                            </tr>
                        #end
                    </tbody>
                </table>
				#pageBarPost($page.totalRow $page.curPage $page.totalPage)
			</div>
        </div>
		#parse("/businessInfo/detailModal.vm")
    </body>
</html>
