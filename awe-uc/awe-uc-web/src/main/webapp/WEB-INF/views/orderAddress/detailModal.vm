<!-- Modal -->
<div class="modal fade" id="myOrderAddressModal" tabindex="-1" role="dialog" aria-labelledby="myOrderAddressModalLabel" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
				<h4 class="modal-title" id="myOrderAddressModalLabel">收货地址表详情</h4>
			</div>
			<div class="modal-body"> 
				<table class="table table-striped table-bordered table-condensed table-hover">
					<tbody>
						<tr>
							<th class="center" style='width:25%;'>自增</th><td id="r_id" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>用户ID</th><td id="r_userNo" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>收货人姓名</th><td id="r_addName" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>省份名称</th><td id="r_addProvinceName" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>省份编码</th><td id="r_addProvinceNo" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>市名称</th><td id="r_addCityName" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>市编号</th><td id="r_addCityNo" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>区名称</th><td id="r_addAreaName" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>区编码</th><td id="r_addAreaNo" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>详细地址</th><td id="r_addAddress" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>移动电话</th><td id="r_addMobile" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>固定电话</th><td id="r_addTelephone" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>电子邮件</th><td id="r_addEmail" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>邮编</th><td id="r_addZipcode" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>创建人</th><td id="r_createName" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>修改人</th><td id="r_updateName" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>创建时间</th><td id="r_createTime" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>修改时间</th><td id="r_updateTime" style='width:25%;'></td>
						</tr>
						<tr>
							<th class="center" style='width:25%;'>是否有效</th><td id="r_yn" style='width:25%;'></td>
						</tr>
					</tbody>
				</table>
			</div>
		</div><!-- /.modal-content -->
	</div><!-- /.modal-dialog -->
</div><!-- /.modal -->

<script language="javascript">         	
    function toDetail(parameterId){
		$("#tab_online_tbody").html("");
		$("#tab_config_tbody").html("");
		$("#tab_log_tbody").html("");
    	jQuery.ajax({
    		type: "GET",
    		url: basePath + "/orderAddress/detail",
    		dataType : 'json',
    		data: "id=" + parameterId,
    		
    		success: function(data){
    			if(data!=null && data.code==200 && data.result!=null){
    				var r = data.result;
    				$("#r_id").text(r.id==null?"":r.id);
    				$("#r_userNo").text(r.userNo==null?"":r.userNo);
    				$("#r_addName").text(r.addName==null?"":r.addName);
    				$("#r_addProvinceName").text(r.addProvinceName==null?"":r.addProvinceName);
    				$("#r_addProvinceNo").text(r.addProvinceNo==null?"":r.addProvinceNo);
    				$("#r_addCityName").text(r.addCityName==null?"":r.addCityName);
    				$("#r_addCityNo").text(r.addCityNo==null?"":r.addCityNo);
    				$("#r_addAreaName").text(r.addAreaName==null?"":r.addAreaName);
    				$("#r_addAreaNo").text(r.addAreaNo==null?"":r.addAreaNo);
    				$("#r_addAddress").text(r.addAddress==null?"":r.addAddress);
    				$("#r_addMobile").text(r.addMobile==null?"":r.addMobile);
    				$("#r_addTelephone").text(r.addTelephone==null?"":r.addTelephone);
    				$("#r_addEmail").text(r.addEmail==null?"":r.addEmail);
    				$("#r_addZipcode").text(r.addZipcode==null?"":r.addZipcode);
    				$("#r_createName").text(r.createName==null?"":r.createName);
    				$("#r_updateName").text(r.updateName==null?"":r.updateName);
    				$("#r_createTime").text(r.createTime==null?"":r.createTime);
    				$("#r_updateTime").text(r.updateTime==null?"":r.updateTime);
    				$("#r_yn").text(r.yn==null?"":r.yn);
    			} else {
    				alert("获取详情失败");
    			}
    		},
    		
    		error : function(data) {
    			alert("获取详情出现异常");
    		}
    	});
		
    	$('#myOrderAddressModal').modal('show');
    }
</script>